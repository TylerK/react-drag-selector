{
  "author": "Tyler Kelley <tylerckelley@gmail.com>",
  "name": "react-drag-select",
  "version": "0.0.1",
  "description": "Higher order React component for selecting items in an area by dragging a selection around them. ",
  "main": "./dist/index.js",
  "keywords": [
    "react",
    "react-component",
    "react-drag-select",
    "drag"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --colors",
    "start": "npm run clean && npm run watch",
    "build": "npm run clean && ./node_modules/.bin/babel ./src/app/index.js -o ./dist/index.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.21.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.1",
    "eslint-plugin-import": "^0.8.1",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "node-sass": "^3.4.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "license": "ISC"
}
